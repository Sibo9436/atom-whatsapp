{
  "name": "atom-whatsapp",
  "main": "./lib/atom-whatsapp",
  "version": "0.1.0",
  "description": "Atom integration for whatsapp web",
  "keywords": [
    "whatsapp",
    "chat"
  ],
  "activationCommands": {
    "atom-workspace": "atom-whatsapp:toggle"
  },
  "repository": "https://github.com/Sibo9436/atom-whatsapp",
  "license": "MIT",
  "engines": {
    "atom": ">=1.40.0 <2.0.0"
  },
  "configSchema": {
    "useAtomFont": {
      "title": "Use atom font",
      "description": "",
      "type": "boolean",
      "default": true,
      "order": 1
    },
    "useAtomThemes": {
      "title": "Enable atom theme settings inside whatsapp",
      "description": "atom-whatsapp tries to apply atom colorscheme and font to whatsapp webview. This may not work properly with some themes.",
      "type": "boolean",
      "default": true,
      "order": 2
    }
  },
  "deserializers": {
    "AtomWhatsappView": "deserializeAtomWhatsappView"
  },
  "dependencies": {}
}
